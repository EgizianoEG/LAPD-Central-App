{
  "name": "lapd-central-bot",
  "version": "1.7.2",
  "description": "A discord.js bot designed to be used as a helper to LAPD-themed Roblox roleplay communities.",
  "author": "EgizianoEG",
  "license": "MIT",
  "main": "Source/Main.ts",
  "type": "module",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/EgizianoEG/LAPD-Central-Bot"
  },
  "keywords": [
    "Roblox",
    "ER:LC",
    "LAPD",
    "Dispatch"
  ],
  "scripts": {
    "watch": "npm run watch:dev",
    "watch:dev": "tsx watch --clear-screen=false --ignore ./Build/**/* --ignore ./Source/Typings/**/* --env-file=.env .",
    "start:js": "node ./Build/Main.js",
    "start:ts": "tsx --env-file=.env .",
    "start": "npm run start:ts",
    "lint": "tsc --noEmit && eslint \"Source/**/*.{js,ts}\" && prettier -c --end-of-line auto \"Source/**/*.{js,ts,json}\"",
    "build": "tsc && tsconfig-replace-paths --project tsconfig.json && npm run cp:sf-to-b",
    "clean-build": "rm -rf Build & npm run build",
    "cp:secrets": "cp ./Source/Config/Secrets.example.ts ./Source/Config/Secrets.ts",
    "cp:sf-to-b": "cp -r ./Source/Resources/Imgs ./Build/Resources && cp -r ./Source/Resources/Fonts ./Build/Resources && cp -r ./Source/Resources/HTML ./Build/Resources && cp -r ./Source/Resources/Libs ./Build/Resources",
    "heroku-prebuild": "npm run cp:secrets",
    "test:coverage": "jest \"--coverage\"",
    "test:secrets": "npm run test -- Tests/Other/Secrets.test.ts",
    "test:utils": "npm run test -- Tests/Utils/*",
    "test:other": "npm run test -- Tests/Other/*",
    "test:rscs": "npm run test -- Tests/Other/Resources.test.ts",
    "test": "jest"
  },
  "dependencies": {
    "@google-cloud/local-auth": "^3.0.1",
    "@isaacs/ttlcache": "^1.4.1",
    "@logtail/node": "^0.5.5",
    "@logtail/winston": "^0.5.5",
    "@napi-rs/canvas": "^0.1.76",
    "axios": "^1.11.0",
    "chalk": "^5.5.0",
    "chrono-node": "^2.8.3",
    "convert-units": "^2.3.4",
    "date-fns": "^4.0.0",
    "dedent": "^1.5.3",
    "default-composer": "^0.6.0",
    "didyoumean2": "^7.0.4",
    "discord.js": "^14.23.2",
    "express": "^5.1.0",
    "fuse.js": "^7.1.0",
    "googleapis": "^149.0.0",
    "humanize-duration": "^3.33.0",
    "jsbarcode": "^3.12.1",
    "linkifyjs": "^4.3.2",
    "mongoose": "^8.17.0",
    "noblox.js": "^6.2.0",
    "node-cron": "^4.2.1",
    "node-ffi-rs": "^1.3.0",
    "node-os-utils": "^1.3.7",
    "parse-duration": "^2.1.4",
    "remeda": "^2.28.0",
    "safe-regex": "^2.1.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.23.0",
    "@faker-js/faker": "^10.0.0",
    "@stylistic/eslint-plugin": "^5.2.2",
    "@types/convert-units": "^2.3.9",
    "@types/express": "^5.0.0",
    "@types/humanize-duration": "^3.27.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "@types/node-cron": "^3.0.11",
    "@types/node-os-utils": "^1.3.4",
    "@types/safe-regex": "^1.1.6",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-sonarjs": "^3.0.4",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "tsconfig-replace-paths": "^0.0.14",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "utility-types": "^3.11.0"
  },
  "engines": {
    "node": "^24.1.0",
    "npm": "^11.2.0"
  }
}
