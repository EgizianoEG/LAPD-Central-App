{
  "name": "lapd-central-bot",
  "version": "0.5.0",
  "description": "A discord.js bot designed to be used as a helper to manage and operate role playing LAPD Discord servers.",
  "author": "EgizianoEG",
  "license": "MIT",
  "main": "Source/Main.ts",
  "type": "module",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/EgizianoEG/LAPD-Central-Bot"
  },
  "keywords": [
    "Roblox",
    "ER:LC",
    "LAPD",
    "Dispatch"
  ],
  "scripts": {
    "watch": "npm run watch:dev",
    "watch:dev": "tsx watch --clear-screen=false --ignore ./Build/**/* --ignore ./Source/Typings/**/* .",
    "start:js": "node ./Build/Main.js",
    "start:ts": "tsx .",
    "start": "npm run start:ts",
    "build": "tsc && tsconfig-replace-paths --project tsconfig.json",
    "clean-build": "rm -rf Build & npm run build",
    "lint": "tsc --noEmit && eslint \"Source/**/*.{js,ts}\" && prettier -c --end-of-line auto \"Source/**/*.{js,ts,json}\"",
    "cp:secrets": "cp ./Source/Config/Secrets.example.ts ./Source/Config/Secrets.ts",
    "test:coverage": "jest \"--coverage\"",
    "test:secrets": "npm run test -- Tests/Other/Secrets.test.ts",
    "test:utils": "npm run test -- Tests/Utils/*",
    "test:rscs": "npm run test -- Tests/Other/Resources.test.ts",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^1.5.1",
    "chalk": "^5.3.0",
    "convert-units": "^2.3.4",
    "dedent": "^1.5.1",
    "discord.js": "^14.13.0",
    "humanize-duration": "^3.30.0",
    "mongoose": "^8.0.0",
    "node-cache": "^5.1.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "@types/convert-units": "^2.3.7",
    "@types/humanize-duration": "^3.27.1",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.6",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-sonarjs": "^0.21.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-replace-paths": "^0.0.14",
    "tsx": "^4.5.0",
    "typescript": "^5.2.2",
    "utility-types": "^3.10.0"
  },
  "engines": {
    "node": "^21.0.0",
    "npm": "^10.2.0"
  }
}
